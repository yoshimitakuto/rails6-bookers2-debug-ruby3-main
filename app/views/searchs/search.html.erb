 <table class="table table-hover">
  <% if @range == 'Book' %>
   <h1>Books search for ""</h1>
    <tbody>
     <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.body %></td>
      </tr>
      <% end %>
    </tbody>
  <% else %>
   <h1>User search for ""</h1>
    <tbody>
     <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
      </tr>
      <% end %>
    </tbody>
  <% end %>
 </table>


